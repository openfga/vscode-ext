{
  "comments": {
    "lineComment": "#",
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
  ],
  "indentationRules": {
   
  },
  "onEnterRules": [
    {
      "beforeText": "^model$",
      "action": { "indent": "indent" }
    },
    {
      "beforeText": "^\\s*relations$",
      "action": { "indent": "indent" }
    },
    {
      "beforeText": "^\\s{2}schema 1\\.[01]$",
      "action": { "indent": "outdent" },
    }
  ]
}
