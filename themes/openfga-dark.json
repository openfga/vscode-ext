{
  "$schema": "vscode://schemas/color-theme",
  "include": "./dark_vs.json",
  "semanticHighlighting": false,
  "type": "dark",
  "colors": {
    "editor.background": "#141517",
    "editor.foreground": "#eeffff",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment.openfga"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#737981"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.as.openfga",
        "keyword.define.openfga",
        "keyword.from.openfga",
        "keyword.with.openfga",
        "keyword.model.openfga",
        "keyword.module.openfga",
        "keyword.extend.openfga",
        "keyword.relations.openfga",
        "keyword.schema.openfga",
        "keyword.self.openfga",
        "keyword.type.openfga",
        "keyword.intersection.openfga",
        "keyword.exclusion.openfga",
        "keyword.union.openfga",
        "keyword.condition.openfga",
      ],
      "settings": {
        "foreground": "#AAAAAA"
      }
    },
    {
      "name": "Definition",
      "scope": [
        "variable.parameter.computed.relation.value.openfga"
      ],
      "settings": {
        "foreground": "#FFFFFF",
      }
    },
    {
      "name": "Value",
      "scope": [
        "relation-definition.bracket.delimiter.openfga",
        "computed.tupletouserset.relation.value.openfga",
        "tupleset.tupletouserset.relation.value.openfga",
        "schema.value.openfga",
        "variable.parameter.type.condition.openfga"
      ],
      "settings": {
        "foreground": "#AAAAAA"
      }
    },
    {
      "name": "Type Restrictions",
      "scope": [
        "support.class.type.reference.value.openfga",
        "variable.parameter.type-restrictions.openfga",
        "relation.type-restrictions.value.openfga",
        "type.type-restrictions.value.openfga",
        "wildcard.type-restrictions.value.openfga",
        "type-restrictions.bracket.delimiter.openfga",
        "colon.type-restrictions.delimiter.openfga",
        "comma.type-restrictions.delimiter.openfga",
        "hashtag.type-restrictions.delimiter.openfga"
      ],
      "settings": {
        "foreground": "#CEEC93"
      }
    },
    {
      "name": "Type",
      "scope": ["support.class.module.name.value.openfga", "support.class.type.name.value.openfga", "support.class.entity.name.function.condition.openfga"],
      "settings": {
        "foreground": "#79ED83"
      }
    },
    {
      "name": "Relation",
      "scope": ["entity.name.function.member.relation.name.openfga", "entity.name.function.variable.parameter.name.condition.openfga"],
      "settings": {
        "foreground": "#20F1F5"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    }
  ]
}
